    <entry id="48ee846f-5f68-d83d-4f9a-e1a797dc1cd1" name="Aucteller" points="35.0" categoryId="456c6974657323232344415441232323" type="model" minSelections="0" maxSelections="-1" minInForce="0" maxInForce="1" minInRoster="0" maxInRoster="-1" minPoints="0.0" maxPoints="-1.0" collective="false" hidden="false" book="HH4: Conquest" page="296">
