    <profile id="06710a9d-a2a8-638f-91b0-2af2fb3e95c2" profileTypeId="57617267656172204974656d23232344415441232323" name="Armoured Cockpit" hidden="false" book="HH:LACAL" page="88">
      <characteristics>
        <characteristic characteristicId="4465736372697074696f6e23232344415441232323" name="Description" value="May ignore Crew shaken or stunned on a D6 roll of 4+"/>
      </characteristics>
      <modifiers/>
    </profile>
