    <profile id="59304e4a-18c6-1283-455b-f79c559b207f" profileTypeId="57617267656172204974656d23232344415441232323" name="Cataphractii Terminator Armour" hidden="false" book="HH:LACAL" page="91">
      <characteristics>
        <characteristic characteristicId="4465736372697074696f6e23232344415441232323" name="Description" value="Affords a 2+ armour save and 4++ Invulnerable save, Slow and Purposeful, Bulky, may not perform sweeping advance.   Models in a unit joined by a model in Cataphractii Terminator Armour do not have the Slow and  Purposeful rule conferred on them, contrary to the usual application of the rule, but must remain in  coherency with the joined character while they stay with the unit (which may limit their distance  moved overall, consolidation moves, etc.)"/>
      </characteristics>
      <modifiers/>
    </profile>
    <profile id="c3bb10b0-0ad1-3d7e-5b6e-20b2f57fbaba" profileTypeId="57617267656172204974656d23232344415441232323" name="Chaff/Flare Launchers" hidden="false" book="HH:LACAL" page="90">
      <characteristics>
        <characteristic characteristicId="4465736372697074696f6e23232344415441232323" name="Description" value="Gains 4++ invulnerable against damage caused by missile weapons. "/>
