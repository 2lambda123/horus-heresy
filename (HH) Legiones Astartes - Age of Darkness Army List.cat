            <entry id="9be06372-f9f8-5fe6-aa48-29a9beca328b" name="X: Iron Hands" points="0.0" categoryId="(No Category)" type="upgrade" minSelections="0" maxSelections="1" minInForce="0" maxInForce="-1" minInRoster="0" maxInRoster="-1" minPoints="0.0" maxPoints="-1.0" collective="false" hidden="false" book="HH2: Massacre" page="0">
